<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="manifest" href="../pwa/manifest.json">
    <link rel="stylesheet" href="../css/style.css">
    <!-- <link rel="preconnect" href="https://fonts.googleapis.com"> -->
    <!-- <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin> -->
    <!-- <link href="https://fonts.googleapis.com/css2?family=Titillium+Web:wght@200;300;600&display=swap" rel="stylesheet"> -->
    <!-- <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.4/font/bootstrap-icons.css"> -->
    <title>gcol</title>
    <script>
        window.addEventListener("load",() =>{
            if("serviceWorker" in navigator){
                console.log("ajout du service worker");
                navigator.serviceWorker.register("../pwa/sw.js");
                console.log("service worker ajouter");
            }
        })
    </script>
</head>

<body>

    <form class="forms" action="/getDatas" method="post">
        <h1 class="preview"></h1>

        <div class="sec1 sec">
            <div class="input select">
                <div class="icone">
                    <i class="bi bi-telephone"></i>
                </div>
                <select class="champ" name="chif" id="#">
                    <option value="alea">Aléatoire</option>
                    <option class="value" value="76">076</option>
                    <option class="value" value="74">074</option>
                    <option class="value" value="60">060</option>
                    <option class="value" value="77">077</option>
                    <option class="value" value="66">066</option>
                </select>
            </div>

            <div class="input">
                <div class="icone">
                    <i class="bi bi-list-columns-reverse"></i>
                </div>
                <input class="cpt" name="quant" value="1" type="number" placeholder="quantiter">
            </div>

            <div class="input select">
                <div class="icone">
                    <i class="bi bi-pencil"></i>
                </div>
                <select class="champ" name="sep" id="#">
                    <option value=" ">Espace</option>
                    <option value=",">Virgule</option>
                    <option value="_">Underscore</option>
                    <option value="-">Tiret</option>
                    <option value="">Aucun</option>
                </select>
            </div>
        </div>

        <div class="sec2 sec">
            <div class="check">
                <label for="ind">Indicatif</label>
                <input class="champ box" name="ind" checked type="checkbox" id="ind">
            </div>
            <div class="check">
                <label for="zero">Zéro</label>
                <input checked class="champ" name="zero" type="checkbox" id="zero">
            </div>
        </div>

        <div class="btn-submit">
            <button type="submit">Créer une nouvelle liste</button>
        </div>
    </form>
    <script src="../js/main.js"></script>
</body>

</html>